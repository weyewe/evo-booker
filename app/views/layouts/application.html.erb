<!DOCTYPE html>
<html>
<head>
  <title>Booker</title>

    
  <%= stylesheet_link_tag 'http://cdn.sencha.io/ext-4.1.0-gpl/resources/css/ext-all.css', 'application'       %>

	
	<% if Rails.env.production? %>
		<%= javascript_include_tag "#{EXT_41_JS}",
	     "application" %>
	<% else %>
		<%= javascript_include_tag  'ext-all-debug-w-comments',
	     "application" %> 
	<% end %> 
  
  <%= csrf_meta_tag %>
</head>
<body>

	<div style="display:none;">
    <div id="app-header-content">
        <div id="app-logo">
            <div class="logo-top">&#160;</div>
            <div id="logo-body">&#160;</div>
            <div class="logo-bottom">&#160;</div>
        </div>
        <h1>Booker</h1>
        <span id="app-msg" class="x-hidden"></span>
    </div>
  </div>

<%= yield %>

</body>
</html>
